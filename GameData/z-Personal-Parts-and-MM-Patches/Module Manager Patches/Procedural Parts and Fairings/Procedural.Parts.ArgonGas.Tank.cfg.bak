@PART[procedural*Liquid]:NEEDS[NearFuturePropulsion] {
	@MODULE[TankContentSwitcher]
	{
		TANK_TYPE_OPTION
		{
			name = ArgonGas
			dryDensity = 0.1
			costMultiplier = 0.01
			
			RESOURCE 
			{
				name = ArgonGas
				unitsPerT = 1174000
			}
		}
	}
}



